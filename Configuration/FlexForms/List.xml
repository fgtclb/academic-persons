                    <minitems>0</minitems>
                    <size>1</size>
                </config>
            </settings.detailPid>
            <settings.demand.groupBy>
                <label>LLL:EXT:academic_persons/Resources/Private/Language/locallang_be.xlf:flexform.el.groupBy.label</label>
                <config>
                    <type>select</type>
                    <renderType>selectSingle</renderType>
                    <items>
                        <numIndex index="0" type="array">
                            <label>LLL:EXT:academic_persons/Resources/Private/Language/locallang_be.xlf:flexform.el.groupBy.items.none</label>
                            <value></value>
                        </numIndex>
                    </items>
                    <itemsProcFunc>Fgtclb\AcademicPersons\Tca\DemandValues->getGroupByValues</itemsProcFunc>
                </config>
            </settings.demand.groupBy>
            <settings.demand.sortBy>
                <label>LLL:EXT:academic_persons/Resources/Private/Language/locallang_be.xlf:flexform.el.sortBy.label</label>
                <config>
                    <type>select</type>
                    <renderType>selectSingle</renderType>
                    <items>
                        <numIndex index="0" type="array">
                            <label>LLL:EXT:academic_persons/Resources/Private/Language/locallang_be.xlf:flexform.el.sortBy.items.none</label>
                            <value></value>
                        </numIndex>
